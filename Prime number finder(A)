#include<iostream>
using namespace std;

int main(){
// check if the number is prime or not 
    int n;
    int i;
   cout<<"enter any number => ";
   cin>>n;
    for(i=2;i*i<=n;i++){
        if(n%i==0){
            break;  
    }
    }
     if( i*i>n && n>1){
         cout<<" prime ";
     }else{
         cout<<"Not a prime";
     }
    
        return 0;
    }
