#include<iostream>
#include<algorithm>// gcd
using namespace std;
int main(){
int n,m;
    cout<<"enter first number==>";
    cin>>n;
    cout<<"enter second number==>";
    cin>>m;
    int s = min(n,m);
    int j;
    if(m==0){
        cout<<"The hcf of two numbers is "<<n<<endl;
        return 0;
    }
    if(n==0){
        cout<<"The hcf of two numbers is "<<m<<endl;
        return 0;
    }
    if(n==m){
        cout<<"the hcf of two numbers is "<< n <<endl;
        return 0;
    }
    if(m!=0 || n!=0){
    for(j=s; j>=1;j--){
        if(n%j==0 && m%j==0){
         break;
        }
    }

    cout<<"the hcf of two numbers is "<< j <<endl;
    }
return 0;
}
