#include <iostream>
using namespace std;
int main() {
    int power = 1, answer = 0, n;
   cout<<"Enter any number:";
    cin>>n;
    int a=n;
    while(n>0){
        int remainder=n%2;
        n=n/2;
        answer+=(remainder*power);
      power=power*10;
    }
cout<<"The Binary code for "<<a<< " is:"<<answer<<endl;
  
    return 0;
}
